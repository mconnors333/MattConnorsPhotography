<p id="notice"><%= notice %></p>

<h1>Photos</h1>
  <h3><%= link_to 'New Photo', new_photo_path %></h3>
    <% @photos.each do |photo| %>
    <img src="<%= photo.image_address %>"/>
        <p><%= link_to 'Show', photo %> |
        <%= link_to 'Edit', edit_photo_path(photo) %> | 
        <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></p>
    <% end %>
