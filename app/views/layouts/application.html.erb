  <div class="collapse navbar-toggleable-md" id="navbarResponsive">
    <a class="navbar-brand" href="#"><%= link_to "Matthew Connors Photography", root_path, class: "logo" %></a>
    <ul class="nav navbar-nav float-xl-right float-l-right float-md-right">
